  <%- include('partials/head') %>

  <script  src="../js/three.min-129.js"></script>
    <script src="../js/o3dv.min-0.7.17.js"></script>
    <script>
      OV.Init3DViewerElements ();
  </script>
</head>

<body class="container">


    <%- include('partials/header') %>


  <main>
    <div class="title">
      <div class="documentPage" style="position: relative;">
      <button onclick="goBack()" class="btn btn-primary">Back to application documents</button>
    </div>
      <script>
      function goBack() {
        window.history.back();
      }
      </script>
      <div class="card">
        
        <% var m = "/models/" + model[0].filename %>
        <% if (model[1]){var m = m + ",/models/" + model[1].filename }%>
       
    <p> Use the scroll wheel on your mouse (or multitouch gestures on a touchscreen) to zoom in and out of the model</p>  
    <p> Hold the left mouse button and move the cursor to rotate the camera</p>
    <p> Hold the right mouse button and move the cursor to pan the camera</p>
    </div>
    <div class="online_3d_viewer"
    style="width: 100%; height: 60vh;"
    model="<%= m%>"
    defaultcolor="200,0,0"
    backgroundcolor="0xf4efff">
</div>
  </main>


  <%- include('partials/footer') %>